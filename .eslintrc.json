{
  "env": {
    "browser": true
  },
  "overrides": [
    {
      "files": ["*.jsx", "*.js"]
    }
  ],
  "extends": ["plugin:react/recommended", "airbnb"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 10,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "camelcase": [0, { "properties": "never" }],
    "import/prefer-default-export": "off"
  }
}
